[Desktop Entry]
Name=Cosmos
Comment=Display a slideshow of pictures of the cosmos
Exec=@SAVERDIR@/slideshow --location=@SLIDESHOW_COSMOS_DIR@
TryExec=@SAVERDIR@/slideshow
StartupNotify=false
Terminal=false
Type=Application
Categories=Screensaver;
